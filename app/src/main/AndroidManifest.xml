<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vegasaurius">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.vegasaurius.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.vegasaurius.Categorias"></activity>
        <activity android:name="com.example.vegasaurius.plantillaRecetas"></activity>
        <activity android:name="com.example.vegasaurius.RecetaMostrada"></activity>
        <activity android:name="com.example.vegasaurius.RecetasPorCategoria"></activity>
        <activity android:name="com.example.vegasaurius.ModificarReceta"></activity>
        <activity android:name="com.example.vegasaurius.Recetario"></activity>
        <activity android:name="com.example.vegasaurius.Menu"></activity>
        <activity android:name="com.example.vegasaurius.CategoriaMenu"></activity>
        <activity android:name="com.example.vegasaurius.GestionMenus"></activity>
        <activity android:name="com.example.vegasaurius.ListaMenus"></activity>
        <activity android:name="com.example.vegasaurius.ListasCompra"></activity>
        <activity android:name="com.example.vegasaurius.ListaAlimentos"></activity>
        <activity android:name="com.example.vegasaurius.Tuto1"></activity>
    </application>

</manifest>